{
  "name": "scrp-lapd-cad-system",
  "version": "1.0.0",
  "description": "Complete FiveM + Web App CAD System for SCRP LAPD",
  "main": "web-app/server.js",
  "scripts": {
    "start": "cd web-app && npm start",
    "dev": "cd web-app && npm run dev",
    "build": "cd web-app && npm run build",
    "test": "cd web-app && npm test",
    "install-web": "cd web-app && npm install",
    "install-all": "npm install && cd web-app && npm install",
    "clean": "cd web-app && npm run clean",
    "lint": "cd web-app && npm run lint",
    "diagnose": "cd web-app && npm run diagnose",
    "test-cookie-security": "cd web-app && npm run test-cookie-security",
    "fix-discord-oauth": "cd web-app && npm run fix-discord-oauth",
    "generate-secrets": "cd web-app && npm run generate-secrets",
    "test-logout": "cd web-app && npm run test-logout",
    "test-analytics": "cd web-app && npm run test-analytics",
    "create-sessions": "cd web-app && npm run create-sessions",
    "view-users": "cd web-app && npm run view-users",
    "rebuild-db": "cd web-app && npm run rebuild-db",
    "fix-db": "cd web-app && npm run fix-db",
    "test-db": "cd web-app && npm run test-db",
  },
  "workspaces": [
    "web-app"
  ],
  "keywords": [
    "fivem",
    "cad",
    "web-app",
    "sonoran",
    "lapd",
    "roleplay",
    "dispatch",
    "911",
    "emergency"
  ],
  "author": "SCRP LAPD Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/scrp-lapd-cad-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/scrp-lapd-cad-system/issues"
  },
  "homepage": "https://github.com/your-org/scrp-lapd-cad-system#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.5.1",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/redis": "^4.0.11",
